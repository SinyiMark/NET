<Window x:Class="Dojo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dojo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="Main" Loaded="Main_Loaded">
        <TextBox x:Name="input" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="303,208,0,0" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="operation" Content="" HorizontalAlignment="Left" Margin="328,148,0,0" VerticalAlignment="Top" Width="61"/>
        <Button x:Name="submit" Content="Submit" HorizontalAlignment="Left" Margin="328,270,0,0" VerticalAlignment="Top" Width="75" Click="submit_Click"/>
        <Button x:Name="start" Content="start" HorizontalAlignment="Left" Margin="328,91,0,0" VerticalAlignment="Top" Width="75" Click="start_Click"/>

    </Grid>
</Window>
